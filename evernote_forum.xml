<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/"
                       xmlns:moz="http://www.mozilla.org/2006/browser/search/">
  <ShortName>Evernote Forum</ShortName>
  <Description>Evernote Forum Search, adds discussion.evernote.com over HTTPS (POST) to your OpenSearch capable browser.</Description>
  <InputEncoding>UTF-8</InputEncoding>
  <Image width="16" height="16">data:image/png;base64,</Image>
  <Url type="text/html" method="POST" template="https://discussion.evernote.com/index.php">
    <Param name="main_search" value="{searchTerms}"/>
    <Param name="app" value="core"/>
    <Param name="module" value="search"/>
    <Param name="do" value="search"/>
  </Url>
  <moz:SearchForm>https://discussion.evernote.com</moz:SearchForm>
</OpenSearchDescription>
